void fsmTraffic(int status) {
	switch( status ) {
	case 1:
		mode1();
		break;
	case 2:
		mode2();
		break;
	case 3:
		mode3();
		break;
	case 4:
		mode4();
		break;
	default:
		break;
	}
}

